{%
  set classes = [
    dom_id ? 'js-view-dom-id-' ~ dom_id,
  ]
%}

  {{ title_prefix }}
  {{ title }}
  {{ title_suffix }}

  {% if header %}
    {{ header }}
  {% endif %}

<div class="row"> 
  <div class="col-md-3">
    {{ exposed }}
    {{ attachment_before }}
  </div>
  <div class="col-md-9">
    <div class="row view-content">
    {% if rows -%}
      {{ rows }}
    {% elseif empty -%}
      {{ empty }}
    {% endif %}
    </div> 
  </div>  
  
  {{ pager }}

  {{ attachment_after }}
  {{ more }}

  {% if footer %}
    <footer>
      {{ footer }}
    </footer>
  {% endif %}

  {{ feed_icons }}
</div>  